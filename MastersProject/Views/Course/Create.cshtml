@model MastersProject.Models.CourseEntity 

@{
    ViewBag.Title = "Course";

}


<form role="form" id="FCourse">
    <div class="card shadow" mb-4>
        <div class="card-header py-3">
            <div class="card-body">
                <div class="table-responsive">
                    @Html.Label("Course Name")
                    @Html.TextBoxFor(x => x.Coursename, new { @class = "form-control" })
                    @Html.HiddenFor(x => x.Courseid)
                </div>

                <!-- /.card-body -->
                <div class="card-footer">

                    <button type="button" onclick="SaveCourse()" class="btn btn-primary">Save</button>
                    <input type="reset" value="Clear" class="btn btn-primary">

                </div>
            </div>
        </div>

    </div>
</form>








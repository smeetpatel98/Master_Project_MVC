@model MastersProject.Models.CourseEntity
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";

}
<section class="content">

    <div class="row">
        <section class="col-md-5">
            <div class="box box-warning">
                <div class="box-header with-border">
                    <h4 class="box-title">Course Entry</h4>
                </div>
                <div class="messagealert" id="alert_container"></div>
                <div class="box-body">
                    @{
                        Html.RenderPartial("Create", new ViewDataDictionary(Model));
                        //Html.RenderPartial("Create");
                    }
                </div>
            </div>
        </section>
        <section class="col-md-7">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h4>Course List</h4>

                    @{
                        Html.RenderPartial("Manage", new ViewDataDictionary(Model.CourseInfo));
                        //Html.RenderPartial("Manage");
                    }
                </div>
            </div>
        </section>
    </div>
    @section Scripts {
        <script src="~/Scripts/app/Course.js"></script>
        <script type="text/javascript">
            function ConfirmationDialog() {
                if (confirm("Are you sure to continue?"))
                    return true;
                else
                    return false;
            }
        </script>
    }
</section>



@model MastersProject.Models.CountryEntity

@{
    ViewBag.Title = "Country";

}


<form role="form" id="FCountry">
    <div class="card shadow" mb-4>
        <div class="card-header py-3">
            <div class="card-body">
                <div class="table-responsive">
                    @Html.Label("Country Name")
                    @Html.TextBoxFor(x => x.Cname, new { @class = "form-control" })
                    @Html.HiddenFor(x => x.Cid)
                </div>
                
                <!-- /.card-body -->
                <div class="card-footer">

                    <button type="button" onclick="SaveCountry()" class="btn btn-primary">Save</button>
                    <input type="reset"  value="Clear" class="btn btn-primary">
                </div>
            </div>
        </div>

    </div>
</form>






